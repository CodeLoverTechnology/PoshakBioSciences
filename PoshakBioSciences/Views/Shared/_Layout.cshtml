    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Poshak Biosciences</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <!-- Facebook Opengraph integration: https://developers.facebook.com/docs/sharing/opengraph -->
    <meta property="og:title" content="">
    <meta property="og:image" content="">
    <meta property="og:url" content="">
    <meta property="og:site_name" content="">
    <meta property="og:description" content="">

    <!-- Twitter Cards integration: https://dev.twitter.com/cards/  -->
    <meta name="twitter:card" content="summary">
    <meta name="twitter:site" content="">
    <meta name="twitter:title" content="">
    <meta name="twitter:description" content="">
    <meta name="twitter:image" content="">

    <!-- Favicons -->
    <link href="~/LogoPoshakBioScience.ico" rel="icon">
    <link href="~/Apple-Icon-PoshakBioScience.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,700,700i|Raleway:300,400,500,700,800" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/Content/aos.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-icons.css" rel="stylesheet" />
    <link href="~/Content/glightbox.min.css" rel="stylesheet" />
    <link href="~/Content/swiper-bundle.min.css" rel="stylesheet" />

    <!-- Template Main CSS File -->
<link href="~/Content/Style.css" rel="stylesheet" />

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <section id="hero">
        <div class="video-container">
            <video autoplay muted loop>
                <source src="~/Images/VB_PS_1.mp4" type="video/mp4" />
            </video>
            <div class="caption">
                <div class="hero-container container-fluid">
                    <div class="wow fadeIn">
                        <div class="row">
                            <div class="col-6">
                                <div class="hero-logo">
                                    <img class="" src="~/Images/LogoPoshakBioScience.png" alt="Poshak BioScience">
                                </div>
                            </div>
                            <div class="col-6 p-5">
                                <div class="hero-logo">
                                    <h1>Welcome to Poshak BioSciencs</h1>
                                    <h2>We Provide <span class="typed" data-typed-items="NATURALLY IMPROVE AQUACULTURE, MODERN BIOSCIENCES & TECHNOLOGY, INNOVATION FOR THE QUALITY PRODUCTS IN THE FIELD OF AQUACULTURE, FULLY BIOTIC NATURAL QUALITY PRODUCTS AQUACULTURE"></span></h2>
                                    <div class="actions">
                                        <a href="#about" class="btn-get-started">Get Strated</a>
                                        <a href="#services" class="btn-services">Our Services</a>
                                    </div>
                                </div>
                                </div>
                            </div>
                    </div>
                </div>
            </div>
        </div>
        
    </section><!-- End Hero -->
    <header id="header" class="d-flex align-items-center">
        <div class="container d-flex align-items-center justify-content-between">

            <a href="http://www.poshakbiosciences.com/" class="logo mr-auto">
                <img class="img-fluid" src="~/Images/FullBannerPS.png" alt="Poshak BioScience"/>
            </a>
           
            <nav id="navbar" class="navbar">
                <ul>
                    <li><a class="nav-link scrollto active" href="#hero">Home</a></li>
                    <li><a class="nav-link scrollto" href="#about">About</a></li>
                    <li><a class="nav-link scrollto" href="#services">Services</a></li>
                    <li><a class="nav-link scrollto " href="#portfolio">Portfolio</a></li>
                    <li><a class="nav-link scrollto" href="#testimonials">Testimonials</a></li>
                    <li><a class="nav-link scrollto" href="#team">Team</a></li>
                    <li><a class="nav-link scrollto" href="#contact">Contact</a></li>
                </ul>
                <i class="bi bi-list mobile-nav-toggle"></i>
            </nav><!-- .navbar -->

        </div>
    </header><!-- End Header -->
